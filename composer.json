{
    "name": "hnquang112/base-skeleton",
    "authors": [
        {
            "name": "Ho Nhat Quang",
            "email": "hnquang112@yahoo.com.vn",
            "homepage": "http://hnquang.xyz",
            "role": "Developer"
        }
    ],
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*",
        "barryvdh/laravel-debugbar": "^2.2",
        "creativeorange/gravatar": "~1.0",
        "cviebrock/eloquent-sluggable": "^4.0.2",
        "laracasts/flash": "^2.0",
        "nesbot/carbon": "^1.21",
        "darrylkuhn/dialect": "dev-master",
        "barryvdh/laravel-elfinder": "^0.3.7",
        "gloudemans/shoppingcart": "^2.1",
        "barryvdh/laravel-ide-helper": "^2.2",
        "simplesoftwareio/simple-qrcode": "^1.4",
        "anhskohbo/no-captcha": "^2.1",
        "tymon/jwt-auth": "^0.5.9",
        "fideloper/proxy": "^3.1",
        "codesleeve/laravel-stapler": "^1.0",
        "laravel/socialite": "^2.0",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.6",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "laracasts/generators": "^1.1"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Http/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
